<h1>Listing leaders</h1>

<table>
  <thead>
    <tr>
      <th>Position</th>
      <th>Name</th>
      <th>Year</th>
      <th>Major</th>
      <th>Hometown</th>
      <th>Email</th>
      <th>Strange fact</th>
      <th>Interests</th>
      <th>Leadership position</th>
      <th>Img Url</th>
      <th>Graduated</th>
      <th>Contact ok</th>
      <th>Past positions</th>
      <th>Post grad email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @leaders.each do |leader| %>
      <tr>
        <td><%= leader.position %></td>
        <td><%= leader.name %></td>
        <td><%= leader.year %></td>
        <td><%= leader.major %></td>
        <td><%= leader.hometown %></td>
        <td><%= leader.email %></td>
        <td><%= leader.strange_fact %></td>
        <td><%= leader.interests %></td>
        <td><%= leader.leadership_position %></td>
        <td><%= leader.img_url %> </td>
        <td><%= leader.graduated %></td>
        <td><%= leader.contact_ok %></td>
        <td><%= leader.past_positions %></td>
        <td><%= leader.post_grad_email %></td>
        <td><%= link_to 'Show', leader %></td>
        <td><%= link_to 'Edit', edit_leader_path(leader) %></td>
        <td><%= link_to 'Destroy', leader, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Leader', new_leader_path %>
